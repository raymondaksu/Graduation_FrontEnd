>\r\n          </div>\r\n          <input\r\n            name=\"password\"\r\n            type=\"password\"\r\n            placeholder=\"Password\"\r\n            value={formik.values.password}\r\n            onChange={formik.handleChange}\r\n          />\r\n        </div>\r\n        {signInError ? (\r\n          <div className=\"error-message\">{signInError}</div>\r\n        ) : null}\r\n\r\n        <button className=\"btn\" type=\"submit\">\r\n          Sign In\r\n        </button>\r\n        <button className=\"btn\" onClick={refresh}>\r\n          Cancel\r\n        </button>\r\n      </form>\r\n    </div>\r\n  );\r\n}",["84","85"],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\components\\SignUp.js",[],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\reportWebVitals.js",[],["86","87"],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\router\\Router.js",[],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\pages\\Home.js",["88","89"],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\components\\navbar\\Navbar.js",["90","91","92"],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\components\\navbar\\NavbarMenuList.js",[],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\pages\\About.js",[],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\pages\\Contact.js",[],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\components\\card\\Card.js",[],"C:\\Users\\Aksu\\Desktop\\Clarusway_Projects\\Fullstack-BlogApp-Django-React\\blog-app\\src\\pages\\profile\\Profile.js",["93"],"import React, { useState, useEffect } from \"react\";\r\nimport { useHistory } from \"react-router-dom\";\r\